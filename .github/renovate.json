{

  "extends": [
    "config:base",
    ":semanticCommitTypeAll(⬆️)"
  ],
  "enabledManagers": ["poetry"],
  "pip_requirements": {
    "fileMatch": [
      "(^|/)([\\w-]*)requirements\\.(txt|pip)$"
    ]
  },
  "poetry": {
    "fileMatch": [
      "pyproject.toml"
    ]
  },
  "python": {
    "labels": [
      "dependencies",
      "python"
    ],
    "packageRules": []
}
}
